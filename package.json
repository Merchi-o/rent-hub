{
  "private": true,
  "sideEffects": false,
  "scripts": {
    "build": "prisma generate && remix build",
    "dev": "run-p dev:*",
    "dev:remix": "cross-env NODE_ENV=development remix dev",
    "typecheck": "tsc",
    "postinstall": "prisma generate",
    "db:setup": "prisma generate && prisma migrate deploy",
    "db:update": "npx prisma db push",
    "db:inspect": "npx prisma studio",
    "db:run:main": "pscale connect rent-hub main --port 3309",
    "db:run:dev": "pscale connect rent-hub dev --port 3309",
    "start": "cross-env NODE_ENV=production netlify dev"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "@radix-ui/react-switch": "^1.0.2",
    "@remix-run/netlify": "^1.15.0",
    "@remix-run/node": "^1.15.0",
    "@remix-run/react": "^1.15.0",
    "bcryptjs": "^2.4.3",
    "cross-env": "^7.0.3",
    "isbot": "^3.6.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.1",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.15.0",
    "@remix-run/eslint-config": "^1.15.0",
    "@remix-run/serve": "^1.15.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "autoprefixer": "^10.4.14",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.8",
    "prisma": "^4.13.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^4.9.5"
  },
  "engines": {
    "node": ">=14"
  }
}
